<h1>口コミの新規投稿</h1>
<%= form_with model: @post, local: true do |form| %>
  <div class="form-group">
    <%= form.label :content, "内容" %>
    <%= form.text_area :content, rows: 5, class: "form-control", required: true %>
  </div>
  <div>
    <%= form.submit "投稿する", class: "btn btn-outline-primary" %>
<% end %>
